<%

var heading = $1 || "h3";

var output = "<" + heading + ">Chrome incompatibilities</" + heading + ">";

// source URL for incompatibilities
var url = "/en-US/docs/Mozilla/Add-ons/WebExtensions/Chrome_incompatibilities";
var none = "None."

// API is either $0 or the name of the calling page
var slug = env.slug;
var pathComponents = slug.split("/");
if (pathComponents < 1) {
    return;
}
var leafDocument = pathComponents[pathComponents.length-1];
var section = $0 || leafDocument;

var incompatibilities = wiki.page(url, section);

if (incompatibilities) {
  output += incompatibilities;
}
else {
  output += none;
}

%>

<%- output %>
