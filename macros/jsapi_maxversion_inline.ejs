<%
/* one parameter: Gecko version */
/* get the page language */
var lang = env.locale;
var str = "";
 
switch(lang) {
    case "ja":
        str = "JSAPI " + $0 + " まで";
        break;
    case "ru":
        str = "JSAPI " + $0 + " и старее";
        break;
    case "uk":
        str = "JSAPI " + $0 + " та старше";
        break;
    default:
        str = "JSAPI " + $0 + " and earlier";
        break;
}


%><span class="standardNoteInline"><%=str%></span>
