          <ol>
            <li><a href="<%=baseURL%>Basics"><%=text['IMSC_basics']%></a></li>
            <li><a href="<%=baseURL%>Using_the_imscJS_polyfill"><%=text['Using_the_imscJS_polyfill']%></a></li>
