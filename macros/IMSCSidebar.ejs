            <li><a href="<%=baseURL%>Mapping_video_time_codes_to_IMSC"><%=text['Mapping_video_time_codes_to_IMSC']%></a></li>
            <li><a href="<%=baseURL%>IMSC_and_other_standards"><%=text['IMSC_and_other_standards']%></a></li>
          </ol>
      </details>
  </li>
</ol>

</section>
