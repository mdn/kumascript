<%
/* Please update this interface to use the IFSummary template. */
var scriptable = "";

switch(string.tolower($4)) {
  case ("yes"):
  case ("oui"):
  case ("так"):
    scriptable = "scriptable";
    break;
  case ("no"):
  case ("non"):
  case ("ні"):
    scriptable = "non scriptable";
    break;
}

var gecko = string.replace(string.replace(string.replace(string.tolower($3), "gecko" , "") ,"mozilla", "") ," ", ""); 
%>
<%- template("IFSummary", [$1, "", scriptable, gecko, ""]) %>
