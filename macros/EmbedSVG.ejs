<%
var found,
    width  = $1 > 0 ? $1 : 250,
    height = $2 > 0 ? $2 : 250,
    s_title = mdn.localString({
        "en-US": 'Embeded SVG..'
    });

env.files.forEach(function (aFile) {
    if(!found && $0 === aFile.filename) {
        found = true;
%>
<iframe title="<%- s_title %>" src="<%- aFile.url %>" width="<%- width %>" height="<%- height %>" frameborder="0" sandbox="allow-scripts"></iframe>
<%
    }
});

if (!found) {
%><p>Unable to find file <em><%= $0 %></em></p><%
}
%>
